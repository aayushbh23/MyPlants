{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Database": {
     // "Provider": "Sqlite" // For SQLite
    "Provider": "SqlServer" // For MS SQL Server
  },
  "ConnectionStrings": {
    "Sqlite": "Data Source=MyPlants.db",
    "SqlServer": "Server={SERVER};Database={DATABASE};User Id={USERNAME};Password={PASSWORD};TrustServerCertificate=True;MultipleActiveResultSets=True"
  },
  "AllowedHosts": "*"
}
